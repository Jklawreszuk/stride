<ResourceDictionary
  xmlns="https://github.com/avaloniaui"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  xmlns:view="clr-namespace:Stride.Editor.Preview.View">

  <ControlTheme x:Key="{x:Type view:StridePreviewView}" TargetType="{x:Type view:StridePreviewView}" >
    <Setter Property="Template">
      <Setter.Value>
        <ControlTemplate TargetType="{x:Type view:StridePreviewView}">
          <ContentPresenter x:Name="PART_StrideView" />
        </ControlTemplate>
      </Setter.Value>
    </Setter>
  </ControlTheme>
</ResourceDictionary>
